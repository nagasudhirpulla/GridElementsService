<link rel="stylesheet" href="~/lib/vanilla-multiselect/vanillaSelectBox.css">
<script src="~/lib/vanilla-multiselect/vanillaSelectBox.js"></script>
<script>
    function setMultiSelValue(selObj) {
        let result = [];
        Array.from(selObj.options).forEach(function (x) {
            if (x.selected) {
                result.push(x.value);
            }
        });
        let sibling = selObj.nextElementSibling;
        while (sibling) {
            if (sibling.tagName === 'INPUT' && sibling.type === 'hidden') {

                break;
            }
            sibling = sibling.nextElementSibling;
        }
        if (sibling) {
            var inpEl = sibling;
            inpEl.value = result;
        }
    }

</script>